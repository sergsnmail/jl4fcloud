<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.sergsnmail.client.ClientController">
      <BorderPane BorderPane.alignment="CENTER">
         <center>
            <Pane minHeight="75.0" prefHeight="80.0" prefWidth="464.0" BorderPane.alignment="CENTER">
               <children>
                  <ProgressBar fx:id="downloadProgress" layoutX="67.0" layoutY="35.0" prefHeight="17.0" prefWidth="389.0" progress="0.0" />
                  <Label fx:id="downloadFileName" layoutX="67.0" layoutY="11.0" text="Label" />
                  <Label fx:id="downloadPackage" layoutX="67.0" layoutY="57.0" text="Label" />
               <ImageView fitHeight="71.0" fitWidth="46.0" layoutX="11.0" layoutY="19.0" pickOnBounds="true" preserveRatio="true">
                  <image>
                     <Image url="@download.png" />
                  </image>
               </ImageView>
               </children>
               <BorderPane.margin>
                  <Insets />
               </BorderPane.margin>
            </Pane>
         </center>
      </BorderPane>
   <BorderPane>
      <center>
         <Pane minHeight="75.0" prefHeight="80.0" prefWidth="464.0" BorderPane.alignment="CENTER">
            <children>
               <ProgressBar fx:id="uploadProgress" layoutX="67.0" layoutY="35.0" prefHeight="17.0" prefWidth="389.0" progress="0.0" />
               <Label fx:id="uploadFileName" layoutX="67.0" layoutY="11.0" text="Label" />
               <Label fx:id="uploadPackage" layoutX="67.0" layoutY="57.0" text="Label" />
               <ImageView fitHeight="71.0" fitWidth="46.0" layoutX="11.0" layoutY="19.0" pickOnBounds="true" preserveRatio="true">
                  <image>
                     <Image url="@upload.png" />
                  </image>
               </ImageView>
            </children>
            <BorderPane.margin>
               <Insets />
            </BorderPane.margin>
         </Pane>
      </center>
   </BorderPane>
      <Button fx:id="SelectSyncDir" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#OnPressedAction" prefHeight="39.0" prefWidth="500.0" text="Select sync folder..." VBox.vgrow="ALWAYS" />
</VBox>
